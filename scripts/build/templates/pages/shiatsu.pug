extends ../layouts/layout.pug

block meta
  meta(property='og:image', content='/assets/shiatsu.png')
  meta(name='twitter:card', content='summary_large_image')
  meta(
    name='twitter:image',
    content='https://clairefro.dev/assets/shiatsu.png'
  )
  meta(name='twitter:image:alt', content='Relaxing shiatsu in Brooklyn')

block head
  link(rel='stylesheet', href='/styles/main.css')
  title Relaxing shiatsu in Brooklyn - 60mins
  meta(name='robots', content='noindex, nofollow')
  meta(property='og:image', content='/assets/social.png', override)
  meta(
    name='twitter:image',
    content='https://clairefro.dev/assets/social.png',
    override
  )
  meta(
    name='twitter:image:alt',
    content='Relaxing shiatsu in Brookyln',
    override
  )
  meta(
    http-equiv='refresh',
    content='0; url=https://calendar.app.google/CBYNqsmGQ8YPXpVz6'
  )

block content
  p Redirecting to booking page...
  p
    a(href='https://calendar.app.google/CBYNqsmGQ8YPXpVz6') Click here if you are not redirected automatically.
  script.
    window.location.href = 'https://calendar.app.google/CBYNqsmGQ8YPXpVz6';
